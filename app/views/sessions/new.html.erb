<div class="container">
  <br>
  <br>
  <br>
  <div class="row">
    <div class="col m4 offset-m4 s12 center">
      <h5>Log in</h5>

      <%= form_tag sessions_path do %>
       <div class="field">
         <%= label_tag :email %><br/>
         <%= text_field_tag :email, params[:email] %>
       </div>
       <div class="field">
         <%= label_tag :password %><br/>
         <%= password_field_tag :password %>
       </div>
       <div class="actions"><%= submit_tag "Log in" %></div>
      <% end %>
    </div>
  </div>
</div>
<br>
<br>
<br>
