    <div class="container">
      <section id="teacher-profile-full">
        <!-- Teacher image -->
        <h5><img id="teacher-img" src="<%= @teacher.picture %>" alt="<%= @teacher.first_name %> Picture"></h5>
        <!-- Book Lesson link -->
        <h5><%= link_to 'Book Lesson', new_teacher_lesson_path(@teacher.id) %></h5>
        <!-- Full name -->
        <h5><%= "#{@teacher.first_name} #{@teacher.last_name}" %></h5>
        <!-- Subject -->
        <h5><%= "Subject: #{@teacher.subject}" %></h5>
        <!-- Teaching Since -->
        <h5><%= "Teaching since: #{@teacher.teaching_since}" %></h5>
        <!-- About -->
        <h5> <%= "About #{@teacher.first_name}" %></h5>
        <p><%= "#{@teacher.bio}" %></p>
        <!-- Street Address -->
        <h5> <%= "#{@teacher.first_name}'s address" %></h3>
        <h5><%= "#{@teacher.street_address}, #{@teacher.city}" %></h5>
        <h5><%= "#{@teacher.province}, #{@teacher.postal_code}" %></h5>
        <!-- Google maps -->
        <div class="row">
          <div class="col s12 center">
            <iframe
            width="100%"
            height="500px"
            frameborder="0" style="border:0"
            src="https://www.google.com/maps/embed/v1/place?key=<%= Rails.application.secrets.maps_key %>&q=<%= @teacher.full_address %>" allowfullscreen>
          </iframe>
        </div>
      </div>
    <!-- Availability -->
    <h5> <%= "#{@teacher.first_name}'s availability" %></h3>
    <%= render 'availability_ranges/show' %>
    <!-- Cost -->
    <h5><%= "Cost per lesson: $#{@teacher.price}" %></h5>
    <!-- Book lesson -->
    <h5><%= link_to 'Book Lesson', new_teacher_lesson_path(@teacher.id) %></h5>
  </section>
</div>
